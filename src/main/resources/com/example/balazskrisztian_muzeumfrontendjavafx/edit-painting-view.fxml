<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<GridPane xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.balazskrisztian_muzeumfrontendjavafx.Controllers.EditPaintingController">
<columnConstraints>
    <ColumnConstraints halignment="RIGHT" hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
    <ColumnConstraints hgrow="SOMETIMES" minWidth="100.0" prefWidth="200.0" />
</columnConstraints>
<rowConstraints>
    <RowConstraints minHeight="30.0" prefHeight="30.0" vgrow="SOMETIMES" />
    <RowConstraints minHeight="30.0" prefHeight="30.0" vgrow="SOMETIMES" />
    <RowConstraints minHeight="30.0" prefHeight="30.0" vgrow="SOMETIMES" />
    <RowConstraints minHeight="30.0" prefHeight="30.0" vgrow="SOMETIMES" />
</rowConstraints>
<children>
    <TextField fx:id="tfTitle" GridPane.columnIndex="1">
         <GridPane.margin>
            <Insets left="10.0" right="10.0" />
         </GridPane.margin></TextField>
    <Label text="Név:">
         <GridPane.margin>
            <Insets right="10.0" />
         </GridPane.margin></Label>
    <Label text="Dátum:" GridPane.rowIndex="1">
         <GridPane.margin>
            <Insets right="10.0" />
         </GridPane.margin></Label>
    <Label text="Kiállítva:" GridPane.rowIndex="2">
         <GridPane.margin>
            <Insets right="10.0" />
         </GridPane.margin></Label>
    <Button fx:id="btnEditPainting" minWidth="100.0" mnemonicParsing="false" onAction="#editPaintingClick" prefWidth="200.0" text="Módosítás" GridPane.columnIndex="1" GridPane.hgrow="ALWAYS" GridPane.rowIndex="3">
         <GridPane.margin>
            <Insets left="10.0" right="10.0" />
         </GridPane.margin></Button>
    <Spinner fx:id="spYear" editable="true" GridPane.columnIndex="1" GridPane.rowIndex="1">
        <valueFactory>
            <SpinnerValueFactory.IntegerSpinnerValueFactory max="10000" min="0" />
        </valueFactory>
         <GridPane.margin>
            <Insets left="10.0" />
         </GridPane.margin>
    </Spinner>
    <CheckBox fx:id="cbOnDisplay" mnemonicParsing="false" GridPane.columnIndex="1" GridPane.rowIndex="2">
         <GridPane.margin>
            <Insets left="10.0" />
         </GridPane.margin></CheckBox>
</children>
</GridPane>
